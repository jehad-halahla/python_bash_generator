<?xml version="1.0" ?>
<CommandManual>
	<command>date</command>
	<description>Display the current time in the given FORMAT, or set the system date.</description>
	<version>date (GNU coreutils) 8.32</version>
	<example>date</example>
	<example_output>
Wed Jan 31 05:51:48 PM IST 2024
</example_output>
	<related_commands>cal, hwclock</related_commands>
</CommandManual>
